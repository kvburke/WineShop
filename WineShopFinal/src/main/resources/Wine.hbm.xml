<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

 

  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.example.demo.Wine" table="Wine">

  <id name="idx" column="idx">

<generator class="native"/>



  </id>
  <property name="id" not-null="true" length= "50" />
  
  <property name="url" not-null="true" length= "50" />

  <property name="brand" not-null="true" length= "50" />

  <property name="name" not-null="true" length="50" />
  
  <property name="price" not-null="true" length= "50" />

  <property name="quantity" not-null="true" length= "50" />
    </class>

</hibernate-mapping>
